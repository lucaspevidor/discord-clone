{
  "extends": "next/core-web-vitals",
  "rules": {
    "eol-last": ["error", "always"],
    "import/order": "error",
    "indent": ["error", 2],
    "no-trailing-spaces": "error",
    "prefer-readonly": "off",
    "quotes": [2, "double"],
    "semi": ["error", "always"],
    "space-before-function-paren": [2, "never"],
    "object-curly-spacing": ["error", "always"]
  }
}
